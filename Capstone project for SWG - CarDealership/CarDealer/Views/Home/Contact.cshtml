@{
    ViewBag.Title = "Contact";
}
<h3>@ViewBag.Message</h3>

<div class="container">
    <div class="col-lg-4 form-group" id="contactForm">
        <label>Name</label><br />
        <input type="text" class="form-control" id="ContactName"/><br />
        <label>Email</label><br />
        <input type="text" class="form-control" id="ContactEmail"/><br />
        <label>Phone</label><br />
        <input type="text" class="form-control" id="ContactPhone"/><br />
        <label>Message</label><br />
        <textarea class="form-control" id="ContactMessage"></textarea><br />
        <button type="submit" class="btn btn-default" id="ContactSubmitButton">Submit</button>
    </div>
    <div>
        <div id="map" style="width:300px;height:300px">
        </div>
        <p>
            111 Market Street,<br />
            Louisville, KY, 40202<br /><br />

            Sales: 111-111-1111<br />
            Service: 111-111-1112
        </p>
    </div>
    <script src="~/Scripts/Contact.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?callback=myMap"></script>
</div>